<?xml version="1.0" encoding="UTF-8"?>
<classDefinitions lastModified="2009-12-23T12:12:12" xmlns="http://schemas.meijing.com.cn/mdbs/1/typeDefinitions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.meijing.com.cn/mdbs/1/typeDefinitions typeDefinitions.xsd">
	<class name="Employee" displayName="雇员" base="Person">
		<property name="Job" displayName="cc">
				<restriction base="string">
					<required />
				</restriction>
		</property>
		<property name="company_test_id">
				<restriction base="string" />
		</property>
		<belongs_to name="company_test_id">Company</belongs_to>
	  <designFor></designFor>
	</class>
	<class name="Boss" base="Employee">
		<property name="Job">
				<restriction base="string">
					<defaultValue>boss</defaultValue>
					<minLength>3</minLength>
					<maxLength>13</maxLength>
				</restriction>
		</property>
	</class>

	<class name="Person" displayName="人员" >
	        	<property name="ID1" displayName="cc">
	        		<restriction base="integer">
	        			<defaultValue>0</defaultValue>
	        		</restriction>
	        	</property>
	        	<property name="Name" displayName="cc">
	        		<restriction base="string">
	        			<defaultValue>mfk</defaultValue>
	        			<minLength>3</minLength>
	        			<maxLength>13</maxLength>
	        		</restriction>
	        	</property>
	        	<property name="Name2" displayName="cc">
	        		<restriction base="string">
	        			<defaultValue>mfk</defaultValue>
	        			<length>3</length>
	        		</restriction>
	        	</property>
	        	<property name="Age" displayName="cc">
	        		<restriction base="integer">
	        			<defaultValue>123</defaultValue>
	        			<minValue>3</minValue>
	        			<maxValue>313</maxValue>
	        		</restriction>
	        	</property>
	        	<property name="Day" displayName="cc">
	        		<restriction base="datetime">
	        			<defaultValue>2009-12-12T12:23:23+08:00</defaultValue>
	        			<minValue>2009-12-11T10:23:23+08:00</minValue>
	        			<maxValue>2009-12-13T12:23:23+08:00</maxValue>
	        		</restriction>
	        	</property>
	        	<property name="Mony" displayName="cc">
	        		<restriction base="decimal">
	        			<defaultValue>1.3</defaultValue>
	        			<minValue>1.0</minValue>
	        			<maxValue>3.0</maxValue>
	        		</restriction>
	        	</property>
	        	<property name="IP" displayName="cc">
	        		<restriction base="ipAddress">
	        			<defaultValue>12.12.12.12</defaultValue>
	        		</restriction>
	        	</property>
	        	<property name="MAC" displayName="cc">
	        		<restriction base="physicalAddress">
	        			<defaultValue>12-12-12-12-12-12</defaultValue>
	        		</restriction>
	        	</property>
	        	<property name="Sex" displayName="cc">
	        		<restriction base="string">
	        			<enumeration>
	        				<value>male</value>
	        				<value>female</value>
	        			</enumeration>
	        			<defaultValue>male</defaultValue>
	        		</restriction>
	        	</property>
	        	<property name="Password" displayName="cc">
	        		<restriction base="password">
	        			<defaultValue>mfk</defaultValue>
	        		</restriction>
	        	</property>
	</class>	
	<class name="Company">
		<property name="Name">
				<restriction base="string">
					<defaultValue>Sina</defaultValue>
				</restriction>
		</property>
		<has_many foreignKey="company_test_id" >Employee</has_many>
		<has_one attributeName="boss" >Boss</has_one>
	    <designFor></designFor>
	</class>	
	<designFor/>
</classDefinitions>
